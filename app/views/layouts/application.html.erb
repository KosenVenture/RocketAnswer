<!DOCTYPE html>
<html>
  <head>
    <title>KaitouShare</title>
    <%= stylesheet_link_tag 'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
    <%= csrf_meta_tags %>
  </head>
  <body>
    <div id="header">
      <%= render 'menu/navbar' %>
    </div>

    <div id="main" class="container">
      <%= bootstrap_alert %>
      <%= bootstrap_notice %>

      <%= yield %>
    </div>
  </body>
</html>
